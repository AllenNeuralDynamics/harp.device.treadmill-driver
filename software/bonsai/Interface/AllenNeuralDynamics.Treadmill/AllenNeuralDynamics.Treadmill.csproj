<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Title>AllenNeuralDynamics.Treadmill</Title>
    <Authors></Authors>
    <Copyright>Copyright Â© 2024 Allen Institute for Neural Dynamics</Copyright>
    <PackageProjectUrl>https://github.com/AllenNeuralDynamics/harp.device.treadmill</PackageProjectUrl>
    <RepositoryUrl>https://github.com/AllenNeuralDynamics/harp.device.treadmill</RepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <GeneratePackageOnBuild Condition="'$(Configuration)'=='Release'">true</GeneratePackageOnBuild>
    <Description>Bonsai Library containing interfaces for data acquisition and control of AllenNeuralDynamics Treadmill device.</Description>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageTags>AllenNeuralDynamics Treadmill Bonsai Rx</PackageTags>
    <PackageLicenseExpression></PackageLicenseExpression>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageIcon>icon.png</PackageIcon>
    <PackageOutputPath></PackageOutputPath>
    <TargetFrameworks>net462;netstandard2.0</TargetFrameworks>
    <VersionPrefix>0.2.0</VersionPrefix>
    <LangVersion>9.0</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Bonsai.Harp" Version="3.5.2" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="..\LICENSE" PackagePath="/" />
    <Content Include="..\icon.png" PackagePath="/" />
    <EmbeddedResource Include="..\..\..\..\device.yml" />
  </ItemGroup>

</Project>
